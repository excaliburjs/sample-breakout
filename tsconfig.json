{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "lib": [
      "dom",
      "es5",
      "ES2015.Proxy",
      "es2015.promise",
      "es2015.collection",
      "es2015.iterable",
      "es2015"
    ],
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}